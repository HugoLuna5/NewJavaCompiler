met_For_34: LOADFP          ; Guardar enlace dinamico 
                   LOADSP          ; Inicializar el FP 
                   STOREFP         ;  
                   STOREFP         ; Reestablece el contexto 
                   RET 1           ; Retorna de la llamada, liberando parametros 
met_main_33: LOADFP          ; Guardar enlace dinamico 
                   LOADSP          ; Inicializar el FP 
                   STOREFP         ;  
                   RMEM 1          ; Se declaran 1 variables locales. Reservo lugar. 
                                   ; Comienzo del FOR_35 
                   PUSH 1          ; Apilo un 1 
                   STORE 0         ; Guardo parametro/var local i 
FOR_L1_35: NOP             ;  
                   LOAD 0          ; Cargo parametro/var local i 
                   PUSH 10         ; Apilo un 10 
                   LE              
                   BF FOR_L2_35    
                   LOAD 0          ; Cargo parametro/var local i 
                   PUSH met_printIln_29 ; Preparo para llamar al metodo System.printIln 
                   CALL            
                   LOAD 0          ; Cargo parametro/var local i 
                   PUSH 1          ; Apilo un 1 
                   ADD             
                   STORE 0         ; Guardo parametro/var local i 
                   JUMP FOR_L1_35  
FOR_L2_35: NOP             ;  
                                   ; Fin del FOR_35 
                   FMEM 1          ; Libero las variables locales declaradas dentro de mi bloque 
                   STOREFP         ; Reestablece el contexto 
                   RET 0           ; Retorna de la llamada, liberando parametros 
