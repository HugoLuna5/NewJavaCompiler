met_main_34: LOADFP          ; Guardar enlace dinamico 
                   LOADSP          ; Inicializar el FP 
                   STOREFP         ;  
                   RMEM 3          ; Se declaran 3 variables locales. Reservo lugar. 
                   PUSH 10         ; Apilo un 10 
                   STORE 0         ; Guardo parametro/var local cantidad 
                                   ; Comienzo del FOR_36 
                   PUSH 1          ; Apilo un 1 
                   STORE -1        ; Guardo parametro/var local i 
FOR_L1_36: NOP             ;  
                   LOAD -1         ; Cargo parametro/var local i 
                   LOAD 0          ; Cargo parametro/var local cantidad 
                   LE              
                   BF FOR_L2_36    
                                   ; Comienzo del FOR_37 
                   PUSH 1          ; Apilo un 1 
                   STORE -2        ; Guardo parametro/var local j 
FOR_L1_37: NOP             ;  
                   LOAD -2         ; Cargo parametro/var local j 
                   LOAD -1         ; Cargo parametro/var local i 
                   LE              
                   BF FOR_L2_37    
                   PUSH 42         ; Apilo un caracter * 
                   PUSH met_printC_24 ; Preparo para llamar al metodo System.printC 
                   CALL            
                   LOAD -2         ; Cargo parametro/var local j 
                   PUSH 1          ; Apilo un 1 
                   ADD             
                   STORE -2        ; Guardo parametro/var local j 
                   JUMP FOR_L1_37  
FOR_L2_37: NOP             ;  
                                   ; Fin del FOR_37 
                   PUSH met_println_27 ; Preparo para llamar al metodo System.println 
                   CALL            
                   LOAD -1         ; Cargo parametro/var local i 
                   PUSH 1          ; Apilo un 1 
                   ADD             
                   STORE -1        ; Guardo parametro/var local i 
                   JUMP FOR_L1_36  
FOR_L2_36: NOP             ;  
                                   ; Fin del FOR_36 
                   FMEM 3          ; Libero las variables locales declaradas dentro de mi bloque 
                   STOREFP         ; Reestablece el contexto 
                   RET 0           ; Retorna de la llamada, liberando parametros 
